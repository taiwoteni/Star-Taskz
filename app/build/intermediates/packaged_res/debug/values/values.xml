<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="highlightColor">#46109E15</color>
    <color name="primary">#FF000000</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="secondary">#8A000000</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="tertiary">#FAF7F7F3</color>
    <color name="themeColor">#FF109E15</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="app_icon_size">80.0dp</dimen>
    <dimen name="extra_large_font_size">26.0sp</dimen>
    <dimen name="extra_large_icon_size">65.0dp</dimen>
    <dimen name="extra_large_value_size">32.0sp</dimen>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="large_font_size">22.0sp</dimen>
    <dimen name="large_icon_size">50.0dp</dimen>
    <dimen name="large_value_size">20.0sp</dimen>
    <dimen name="medium_font_size">16.0sp</dimen>
    <dimen name="medium_icon_size">30.0dp</dimen>
    <dimen name="medium_value_size">16.0sp</dimen>
    <dimen name="regular_font_size">14.0sp</dimen>
    <dimen name="regular_icon_size">20.0dp</dimen>
    <dimen name="regular_value_size">8.0sp</dimen>
    <dimen name="small_font_size">12.0sp</dimen>
    <dimen name="small_icon_size">16.0dp</dimen>
    <dimen name="small_value_size">4.0sp</dimen>
    <string name="app_name">Star Taskz</string>
    <string name="first_fragment_label">First Fragment</string>
    <string name="hello_first_fragment">Hello first fragment</string>
    <string name="hello_second_fragment">Hello second fragment. Arg: %1$s</string>
    <string name="next">Next</string>
    <string name="previous">Previous</string>
    <string name="second_fragment_label">Second Fragment</string>
    <string name="title_activity_login">LoginActivity</string>
    <style name="BottomBarTheme" parent="Theme.AppCompat.Light">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">10sp</item>
        <item name="menuGravity">center</item>
    </style>
    <style name="Theme.Taskz" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">@color/white</item>
        <item name="statusBarForeground">@color/white</item>

        
    </style>
    <style name="Theme.Taskz.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.Taskz.Fullscreen" parent="Theme.AppCompat.NoActionBar">

        <item name="android:statusBarColor">@color/white</item>
        <item name="android:navigationBarColor">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Theme.Taskz.NoActionBar" parent="Theme.AppCompat.NoActionBar">
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:navigationBarColor">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="windowActionBar">false</item>
    </style>
    <style name="Theme.Taskz.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <declare-styleable name="LoadableButton">
        <attr format="boolean" name="loading"/>
        <attr format="string" name="text"/>
        <attr format="color" name="textColor"/>
        <attr format="color" name="backgroundColor"/>
    </declare-styleable>
    <declare-styleable name="TextInputFormField">
        <attr format="string" name="hint"/>
        <attr format="color" name="hintColor"/>
        <attr format="string|reference" name="fontFamily"/>
        <attr format="reference" name="prefixIcon"/>
        <attr name="android:inputType"/>
        <attr format="boolean" name="isPassword"/>
    </declare-styleable>
</resources>